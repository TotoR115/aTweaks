IF
	!GlobalTimerNotExpired("RR#wwolf","LOCALS")
	See([PC.0.0.MAGE_ALL])
	Range(LastSeenBy(Myself),10)
	!Range(LastSeenBy(Myself),4)
	CheckStatLT(LastSeenBy(Myself),50,RESISTMAGIC)
	CheckStatLT(LastSeenBy(Myself),75,RESISTCOLD)
	!CheckStatGT(LastSeenBy(Myself),0,MINORGLOBE)
	!CheckStat(LastSeenBy(Myself),6,WIZARD_SPELL_IMMUNITY)
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
		SetGlobalTimer("RR#wwolf","LOCALS",54)
		ForceSpellRES("rr#frost",LastSeenBy(Myself))  // frost breath
		SetInterrupt(TRUE)
END

