IF
    Allegiance(Myself,EVILCUTOFF)
	CheckStatGT(Myself,15,LEVEL)
	!GlobalTimerNotExpired("rr#suffo","LOCALS")
	See([GOODCUTOFF.HUMANOID])
	InWeaponRange(LastSeenBy(Myself))
	OR(2)
		CheckStatGT(LastSeenBy(Myself),0,ENTANGLE)
		StateCheck(LastSeenBy(Myself),STATE_IMMOBILE)
	!StateCheck(LastSeenBy(Myself),STATE_REALLY_DEAD)
	CheckStatGT(LastSeenBy(Myself),0,SAVEVSDEATH)
	!CheckStatGT(LastSeenBy(Myself),0,DEATH_WARD)
	!Check%State%(LastSeenBy(Myself),%Num%ITEM_SLAY)
THEN
	RESPONSE #100
        SetInterrupt(FALSE)
		SetGlobalTimer("rr#suffo","LOCALS",12)
		ForceSpellRES("rr#suffo",LastSeenBy(Myself))  // suffocate
        SetInterrupt(TRUE)
END

